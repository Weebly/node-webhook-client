{
    "manifest": "1",
    "client_id": "{{YOUR_CLIENT_ID}}",
    "version": "1.0.0",
    "callback_url": "https://{{SERVER_ROOT_URL}}/oauth/phase-one",
    "scopes": [
        "read:user",
        "read:site"
    ],
    "oauth_final_destination": "editor",
    "webhooks": {
        "callback_url": "https://{{SERVER_ROOT_URL}}/webhooks/callback",
        "events": ["user.update","site.publish", "site.delete", "app.uninstall"]
    }
}
